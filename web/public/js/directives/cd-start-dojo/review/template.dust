<form class="cd-sad-review" >
  <div class="cd-sad-review-step" ng-repeat="(key, step) in $ctrl.steps" ui-sref="{{ step.state }}">
    <h3>{{ step.name }}
      <span class="cd-sad-review-step__icon">
        <i class="fa"
          ng-class="$ctrl.application[key].isValid ? 'fa-check cd-icon--success': 'fa-times cd-icon--danger'">
        </i>
      </span>
    </h3>
    <i class="fa fa-angle-right pull-right cd-sad-review-step__icon-navigate h3"></i>
  </div>
</form>
